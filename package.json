{
  "name": "graphqltypescript",
  "version": "1.0.0",
  "description": "GraphQL with Typescript backend",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pastor59/graphqlTypescript"
  },
  "main": "index.js",
  "scripts": {
    "start": "node ./build/src/index.js",
    "build": "tsc",
    "build:start": "npm run build && npm run start",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "eslint": "eslint ./**/*.ts",
    "eslint:fix": "eslint ./**/*.ts --fix"
  },
  "keywords": [
    "graphql",
    "typescript"
  ],
  "author": "Jordi Pastor",
  "license": "ISC",
  "dependencies": {
    "apollo-datasource": "^3.3.0",
    "apollo-server-express": "^3.5.0",
    "bcrypt": "^5.0.1",
    "crypto-hash": "^2.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-jwt": "^6.1.0",
    "graphql": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.3",
    "object-hash": "^2.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express-jwt": "^6.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.11.12",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  }
}
